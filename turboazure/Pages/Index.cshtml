@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
    List<Car>? cars = ViewData["BlobURLs"] as List<Car>; ;
}

<div class="text-center">
    <div class="row">
        @foreach (var car in cars)
        {
            <div class="card" style="width: 18rem;">
                <img class="card-img-top" src=@(car.ImageUrl) alt="Card image cap">
                <div class="card-body">
                    <h5>@(car.Brand)</h5>
                    <h6 class="card-text">@(car.Model) @(car.Year)</h6>
                </div>
            </div>
        }
    </div>
</div>
